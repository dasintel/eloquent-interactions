{
    "name": "zachflower/eloquent-interactions",
    "description": "An implementation of the interactor pattern for Laravel.",
    "keywords": ["interactor", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zachary Flower",
            "email": "zach@zacharyflower.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/validation": "5.3.*|~5.4.*|~5.5.*|~5.6.*",
        "illuminate/support": "5.3.*|~5.4.*|~5.5.*|~5.6.*",
        "illuminate/console": "5.3.*|~5.4.*|~5.5.*|~5.6.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "EloquentInteractions\\": "src/EloquentInteractions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EloquentInteractions\\EloquentInteractionsServiceProvider"
            ]
        }
    }
}
